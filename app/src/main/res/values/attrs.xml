<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CalendarView">
        <attr name="cv_weekTextSize" format="dimension|reference" />
        <attr name="cv_dateHeight" format="dimension|reference" />
        <attr name="cv_dateTextSize" format="dimension|reference" />
        <attr name="cv_dateTextColor" format="color|reference" />
        <attr name="cv_dateMargin" format="dimension|reference" />
        <attr name="cv_dateBottomTextSize" format="dimension|reference" />
        <attr name="cv_dateCircleSize" format="dimension|reference" />
